		<!-- BEGIN CONTAINER -->
		<div class="container wide">
			<div class="page-container row-fluid">
				<div class="page-content">
					<div class="content">
						<div class="row">
							<div class="col-md-3">
								<div class="tile white m-b-10 padding-20">
								<% if(req.options.controller=='report') { %>
									<div class="tile-title">
										<h3 class="m-t-0 m-b-10"><%- req.session.getTranslation('Reports_and_statistics') %></h4>
										<%- toolsService.getFlashMessage(req.flash()) %>
									</div>
									<div class="tile-body">
										<ul class="account-menu">
											<li>
												<h5><a class="<%- (req.options.action=='orders' ? 'selected' : '') %>" href="/reports/orders"><%- req.session.getTranslation('Order_history') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='transactions' ? 'selected' : '') %>" href="/reports/transactions"><%- req.session.getTranslation('Transaction_history') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='events' ? 'selected' : '') %>" href="/reports/events"><%- req.session.getTranslation('Events') %></a></h5>
											</li>
										</ul>
									</div>
								<% } else if(req.options.controller=='account' && section=='agency') { %>
									<div class="tile-title">
										<h3 class="m-t-0 m-b-10"><%- req.session.getTranslation('Account_admin') %></h4>
										<%- toolsService.getFlashMessage(req.flash()) %>
									</div>
									<div class="tile-body">
										<ul class="account-menu">
											<li>
												<h5><a class="<%- (req.options.action=='profile' ||req.options.action=='edit_profile' || req.options.action=='change_password' ? 'selected' : '') %>" href="/account/profile"><%- req.session.getTranslation('Profile') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='agency_view' ? 'selected' : '') %>" href="/account/agency_view"><%- req.session.getTranslation('Agency_details') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='agency_clients' ? 'selected' : '') %>" href="/account/agency_clients"><%- req.session.getTranslation('Agency_clients') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='agency_users' ? 'selected' : '') %>" href="/account/agency_users"><%- req.session.getTranslation('Agency_brokers') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='agency_warranties' ? 'selected' : '') %>" href="/account/agency_warranties"><%- req.session.getTranslation('Warranties') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='notifications' ? 'selected' : '') %>" href="/account/notifications"><%- req.session.getTranslation('Messages') %></a></h5>
											</li>
										</ul>
									</div>
								<% } else if(req.options.controller=='account' && section=='assets') { %>
									<div class="tile-title">
										<h3 class="m-t-0 m-b-10"><%- req.session.getTranslation('Assets') %></h4>
										<%- toolsService.getFlashMessage(req.flash()) %>
									</div>
									<div class="tile-body">
										<ul class="account-menu">
											<li>
												<h5><a class="<%- (req.options.action=='agency_assets' ? 'selected' : '') %>" href="/account/agency_assets"><%- req.session.getTranslation('Agency_assets') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='market_assets' ? 'selected' : '') %>" href="/account/market_assets"><%- req.session.getTranslation('Market_assets') %></a></h5>
											</li>
											<li>
												<h5><a class="<%- (req.options.action=='today_assets' ? 'selected' : '') %>" href="/account/today_assets"><%- req.session.getTranslation('Assets_today') %></a></h5>
											</li>
										</ul>
									</div>
								<% } %>
								</div>
							</div>
							<div class="col-md-9">
								<%- body %>
							</div>
						</div>
					</div>
				</div>
			</div>

		</div>
