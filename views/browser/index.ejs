<div class="login-container">
  <div class="row">
    <div class="col-md-12 col-xs-12 text-center">
      <a href="http://fonduri-ue.ro" target="_blank" title="Ministerul Fondurilor Europene"><img src="/img/logo-ue.png" class="logo-ue" /></a>
      <p class="text-black m-t-20 m-b-5"><%- req.session.getTranslation('Program_operational_sectorial') %></p>
      <p class="text-black m-b-5"><%- req.session.getTranslation('Proiect_cofinantat') %></p>
      <div class="text-center m-t-10 m-b-10"><img src="/img/logo-disponibil.png" /></div>
    </div>
  </div>
  <div class="row">
    <div class="col-md-10 col-md-offset-1 col-xs-12">
      <div class="tile">
        <div class="tile-title m-t-0 p-l-5">
          <h4 class="bold"><%- req.session.getTranslation('Browser_not_supported') %></h4>
        </div>
        <h5><%- req.session.getTranslation('Recommended_browsers') %></h5>
        <div class="row browser-section">
          <div class="col-md-4 browser">
            <a href="https://www.google.com/chrome/" title="Chrome" target="_blank">
              <img src="/img/logo-chrome.png" alt="Chrome" />
              <br />Chrome
            </a>
          </div>
          <div class="col-md-4 browser">
            <a href="https://www.mozilla.org/en-US/firefox/new/" title="Firefox" target="_blank">
              <img src="/img/logo-firefox.png" alt="Firefox" />
              <br />Firefox
            </a>
          </div>
          <div class="col-md-4 browser">
            <a href="http://www.opera.com/download" title="Opera" target="_blank">
              <img src="/img/logo-opera.png" alt="Opera" />
              <br />Opera
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6 col-md-offset-3 col-xs-12 text-center">
      <p class="text-white m-t-20 m-b-10"><%- req.session.getTranslation('Informatii_fonduri_ue') %></p>
      <p class="text-white m-b-20"><%- req.session.getTranslation('Continut_material') %></p>
    </div>
  </div>
</div>
