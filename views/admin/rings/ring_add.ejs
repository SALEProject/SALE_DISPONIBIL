																<li><a href="/admin/reports/assets/orders/<%= asset.ID %>">Ordine</a></li>
																<li><a href="/admin/reports/assets/transactions/<%= asset.ID %>">Tranzactii</a></li>
																<li><a href="/admin/reports/assets/warranties/<%= asset.ID %>">Garantii</a></li>
																<li><a href="/admin/reports/assets/documents/<%= asset.ID %>">Documente</a></li>
																<li><a href="/admin/reports/assets/quotes/<%= asset.ID %>">Cotatii</a></li>
																<li><a href="/admin/reports/assets/tradeparams/<%= asset.ID %>">Parametrii tranzactionare</a></li>
