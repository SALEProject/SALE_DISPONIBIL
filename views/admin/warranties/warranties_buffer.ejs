    <div class="content">
	<div class="page-title">
		<ul class="breadcrumb">
			<li>
				<a href="/admin"><i class="fa fa-home"></i></a>
			</li>
			<li>
				<a href="/admin/warranties" class="active">Buffer Tranzactii</a>
			</li>
		</ul>
	</div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<!--<a href="/admin/warranties/add" class="btn btn-info"><i class="fa fa-plus-circle"></i>&nbsp;Adauga garantie</a>-->
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th style="width:10%">ID Tranzactie Bancara</th>
							<th style="width:10%">Data</th>
							<th style="width:10%">Suma</th>
							<th style="width:10%">Tip Tranzactie</th>
							<th style="width:10%">Furnizor</th>
							<th style="width:10%">Banca furnizor</th>
							<th style="width:10%">Cont furnizor</th>
							<th style="width:10%">Cod fiscal furnizor</th>
							<th style="width:10%">Beneficiar</th>
							<th style="width:10%">Banca beneficiar</th>
							<th style="width:10%">Cont beneficiar</th>
							<th style="width:10%">Cod fiscal beneficiar</th>
							<th style="width:5%">&nbsp;</th>
							<th style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle"><span class="muted"><%= item.BankTransactionID %></span></td>
							<td class="v-align-middle"><span class="muted"><%- item.TransactionDate ? timeService.formatDate(item.TransactionDate,'DD MMM YYYY') : '' %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.TransactionAmount %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.TransactionType %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.SupplierName %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.SupplierBank %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.SupplierBankAccount %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.SupplierFiscalCode %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.BeneficiaryName %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.BeneficiaryBank %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.BeneficiaryBankAccount %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.BeneficiaryFiscalCode %></span></td>
							<td><a href="/admin/warranties/buffer/edit/<%= item.ID %>" class="action-icon btn btn-small btn-success" title="Modifica"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/warranties/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-danger"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>
